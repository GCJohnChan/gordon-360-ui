{
  "name": "gordon-360-ui",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "chart.js": "^2.7.1",
    "classnames": "^2.2.5",
    "history": "^4.7.2",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.4",
    "material-ui": "^1.0.0-beta.22",
    "material-ui-icons": "^1.0.0-beta.17",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.1.2",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-chartjs-2": "^2.6.4",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.2.0",
    "react-dropzone": "^4.2.3",
    "react-ga": "^2.3.5",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "eslint": "^4.12.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "husky": "^0.14.3",
    "jsdoc": "^3.5.5",
    "markdownlint-cli": "^0.5.0",
    "react-scripts": "^1.0.17",
    "remark-cli": "^4.0.0",
    "remark-preset-lint-consistent": "^2.0.1",
    "remark-preset-lint-recommended": "^3.0.1",
    "sass-lint": "^1.12.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all --parallel watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "docs": "jsdoc --configure .jsdoc.json --recurse",
    "lint": "npm-run-all --parallel --aggregate-output --silent lint-js lint-md lint-scss",
    "lint-js": "eslint --ext .js src",
    "lint-md": "remark . --frail",
    "lint-scss": "sass-lint 'src/**/*.scss' --verbose --max-warnings 0",
    "precommit": "npm run lint"
  },
  "proxy": {
    "/api": {
      "target": "https://360ApiTrain.gordon.edu/",
      "changeOrigin": true
    },
    "/token": {
      "target": "https://360ApiTrain.gordon.edu/",
      "changeOrigin": true
    }
  }
}
